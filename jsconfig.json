{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@src/*": [
        "src/*"
      ],
      "@services": [
        "./src/services"
      ],
      "@services/*": [
        "./src/services/*"
      ],
      "@config": [
        "./src/config"
      ],
      "@config/*": [
        "./src/config/*"
      ],
      "@api": [
        "./src/api"
      ],
      "@api/*": [
        "./src/api/*"
      ],
      "@routes": [
        "./src/routes"
      ],
      "@routes/*": [
        "./src/routes/*"
      ],
      "@constants": [
        "./src/config/constants"
      ],
      "@utils": [
        "./src/utils"
      ],
      "@models/*": [
        "./src/models/*"
      ],
      "@middlewares": [
        "./src/middlewares"
      ]
    }
  },
  "exclude": [
    "node_modules"
  ]
}